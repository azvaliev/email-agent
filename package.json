{
  "name": "email-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "db:migrate": "kysely migrate:latest",
    "db:migrate:up": "kysely migrate:up",
    "db:migrate:down": "kysely migrate:down --all",
    "db:migrate:new": "kysely migrate:make",
    "db:migrate:list": "kysely migrate:list",
    "db:codegen": "kysely-codegen --dialect postgres --out-file src/db/generated/schema.ts"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "kysely": "^0.28.9",
    "kysely-postgres-js": "^3.0.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.0",
    "postgres": "^3.4.7",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "shadcn": "^3.6.2",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.0.5",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "kysely-codegen": "^0.19.0",
    "kysely-ctl": "^0.19.0",
    "pg": "^8.16.3",
    "prettier": "^3",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "packageManager": "yarn@4.12.0"
}
