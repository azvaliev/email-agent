{
  "name": "email-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "check": "concurrently --names \"typecheck,lint,fmt,test\" \"yarn typecheck\" \"yarn lint\" \"yarn fmt:check\" \"yarn test\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:migrate": "kysely migrate:latest",
    "db:migrate:up": "kysely migrate:up",
    "db:migrate:down": "kysely migrate:down --all",
    "db:migrate:new": "kysely migrate:make",
    "db:migrate:list": "kysely migrate:list",
    "db:codegen": "kysely-codegen --dialect postgres --camel-case --out-file src/db/generated/schema.ts"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@tanstack/react-query": "^5.90.12",
    "@trpc/client": "^11.8.0",
    "@trpc/server": "^11.8.0",
    "@trpc/tanstack-react-query": "^11.8.0",
    "better-auth": "^1.4.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "google-auth-library": "^10.5.0",
    "googleapis": "^169.0.0",
    "kysely": "^0.28.9",
    "lucide-react": "^0.562.0",
    "next": "16.1.0",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "postgres": "^3.4.7",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "shadcn": "^3.6.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.0.5",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/jsdom": "^27",
    "@types/node": "^20",
    "@types/pg": "^8",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "1.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "jsdom": "^27.3.0",
    "kysely-codegen": "^0.19.0",
    "kysely-ctl": "^0.19.0",
    "pino-pretty": "^13.1.3",
    "postcss": "^8.5.6",
    "prettier": "^3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "packageManager": "yarn@4.12.0"
}
